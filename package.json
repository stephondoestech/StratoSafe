{
  "name": "stratosafe",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "backend",
    "frontend"
  ],
  "scripts": {
    "start": "concurrently \"yarn workspace stratosafe-backend start\" \"yarn workspace stratosafe-frontend start\"",
    "build": "yarn workspace stratosafe-backend build && yarn workspace stratosafe-frontend build",
    "install-deps": "yarn install"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
  "resolutions": {
    "@types/express-serve-static-core": "4.17.35",
    "@types/express": "4.17.17"
  }
}